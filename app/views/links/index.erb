<h1> List Links </h1>

<% if current_user %>
Welcome <%= current_user.email %>
<% end %>
<ul id='links'>
<% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL: <%= link.url %>
      <ul id='tags'>
      <% link.tags.each do |tag| %>
      <li>
      Tags: <%= tag.name %>
    </li>
    <% end %>
      </ul>
    </li>
  <% end %>
</ul>
